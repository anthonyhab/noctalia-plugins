{
  "id": "swww-picker",
  "name": "SWWW Wallpaper Picker",
  "version": "0.2.3",
  "minNoctaliaVersion": "3.6.0",
  "author": "habibe",
  "license": "MIT",
  "repository": "https://github.com/anthonyhab/noctalia-plugins",
  "description": "Cycle wallpapers using swww with manual and automatic controls.",
  "tags": ["Bar", "Desktop", "System"],
  "entryPoints": {
    "main": "Main.qml",
    "panel": "Panel.qml",
    "barWidget": "BarWidget.qml",
    "settings": "Settings.qml"
  },
  "dependencies": {
    "plugins": []
  },
  "metadata": {
    "allowUserSettings": true,
    "defaultSettings": {
      "wallpapersDir": "~/Pictures/Wallpapers",
      "autoCycleEnabled": false,
      "autoCycleInterval": 30,
      "transitionType": "grow",
      "transitionDuration": 1,
      "transitionFps": 60,
      "transitionStep": 90,
      "shuffleMode": false,
      "showWallpaperName": true,
      "gridColumns": 2
    }
  }
}
